ELEVENLABS_AGENT_ID=<your-agent-id>
ELEVENLABS_API_KEY=<your-api-key>

# Twilio outbound calling
TWILIO_ACCOUNT_SID=<your-account-sid>
TWILIO_AUTH_TOKEN=<your-auth-token>
TWILIO_PHONE_NUMBER=<your-twilio-phone-number>


# For dev, you can use the same key for both frontend and backend
# Frontend key (for browser usage - restricted to specific domains/IPs)
GOOGLE_MAPS_API_KEY_FRONTEND=
# Backend key (for server usage - less restricted but never exposed to client)
GOOGLE_MAPS_API_KEY_BACKEND=

POSTHOG_API_KEY=your_api_key_here
POSTHOG_HOST=https://app.posthog.com 

# Optional - Will fall back to request host if not set
PUBLIC_URL=https://your-domain.com


